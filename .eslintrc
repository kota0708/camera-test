{
	"env": {
		"es6": true,
		"browser": true,
		"jquery": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:prettier/recommended"
	],
	"rules": {
		"no-console": "off",
		"no-undef": "off"
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"allowImportExportEverywhere": true
	}
}